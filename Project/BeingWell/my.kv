                TextInput:
                    pos_hint: {"x":3, "top":3}
                    size_hint: 4,0.5
                    font_size:28
                    id: cause
                    multiline: False
        Button:
            text:"SUBMIT"
            size_hint: 0.4, 0.1
            background_color: 0,1,0,1
            pos_hint: {"x":0.35, "top":0.2}
            on_press:
