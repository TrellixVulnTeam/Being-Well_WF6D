<FirstScreen>:
    name:"first"
    FloatLayout:
        Label:
            text:"BEING WELL"
            font_size:40
            pos_hint: {"x":0.05, "top":1.45}
        Button:
            text:"ENTER"
            background_color: 0,1,0,1
            size_hint: 0.4, 0.1
            pos_hint: {"x":0.35, "top":0.5}
