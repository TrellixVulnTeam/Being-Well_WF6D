            FloatLayout:
                cols:2
                Label:
                    text:"Name:"
                    font_size:28
                    pos_hint: {"x":0.8, "top":5}

                TextInput:
                    pos_hint: {"x":3, "top":4.78}
                    size_hint: 4,0.5
                    font_size:28
